<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <main>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Map</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.addresses>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi2.settings.addresses</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items type="array"></items>
                                <maxitems>100</maxitems>
                                <size>10</size>
                                <minitems>0</minitems>
                                <selectedListStyle>width:300px;</selectedListStyle>
                                <itemListStyle>width:300px;</itemListStyle>
                                <foreign_table>tt_address</foreign_table>
                                <foreign_table_where>AND tt_address.deleted = 0 AND tt_address.hidden = 0 order by tt_address.name</foreign_table_where>
                                <itemsProcFunc>In2code\Osm\Tca\FilterAddresses->filter</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.addresses>
                    <settings.zoom>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:osm/Resources/Private/Language/locallang_db.xlf:pi2.settings.zoom</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,int</eval>
                                <range>
                                    <lower>0</lower>
                                    <upper>18</upper>
                                </range>
                                <default>14</default>
                                <slider>
                                    <step>1</step>
                                    <width>350</width>
                                </slider>
                            </config>
                        </TCEforms>
                    </settings.zoom>
                </el>
            </ROOT>
        </main>
    </sheets>
</T3DataStructure>
